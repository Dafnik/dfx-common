{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "packageManager": "pnpm"
  },
  "newProjectRoot": "libs",
  "projects": {
    "dfx-bootstrap-table": {
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "development": {
              "tsConfig": "libs/dfx-bootstrap-table/tsconfig.lib.json"
            },
            "production": {
              "tsConfig": "libs/dfx-bootstrap-table/tsconfig.lib.prod.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "libs/dfx-bootstrap-table/eslint.config.js",
            "lintFilePatterns": ["libs/dfx-bootstrap-table/**/*.ts", "libs/dfx-bootstrap-table/**/*.html"]
          }
        },
        "test": {
          "builder": "@angular/build:unit-test",
          "options": {
            "tsConfig": "libs/dfx-bootstrap-table/tsconfig.spec.json"
          }
        }
      },
      "prefix": "lib",
      "projectType": "library",
      "root": "libs/dfx-bootstrap-table",
      "sourceRoot": "libs/dfx-bootstrap-table/src"
    },
    "dfx-bootstrap-table-demo": {
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "configurations": {
            "development": {
              "extractLicenses": false,
              "optimization": false,
              "sourceMap": true
            },
            "production": {
              "baseHref": "/bootstrap-table/",
              "budgets": [
                {
                  "maximumError": "1MB",
                  "maximumWarning": "500kB",
                  "type": "initial"
                },
                {
                  "maximumError": "8kB",
                  "maximumWarning": "4kB",
                  "type": "anyComponentStyle"
                }
              ],
              "outputHashing": "all"
            }
          },
          "defaultConfiguration": "production",
          "options": {
            "assets": [
              {
                "glob": "**/*",
                "input": "apps/dfx-bootstrap-table-demo/public"
              }
            ],
            "browser": "apps/dfx-bootstrap-table-demo/src/main.ts",
            "inlineStyleLanguage": "scss",
            "outputMode": "server",
            "server": "apps/dfx-bootstrap-table-demo/src/main.server.ts",
            "ssr": {
              "entry": "apps/dfx-bootstrap-table-demo/src/server.ts"
            },
            "styles": ["apps/dfx-bootstrap-table-demo/src/styles.scss", "node_modules/bootstrap/dist/css/bootstrap.min.css"],
            "tsConfig": "apps/dfx-bootstrap-table-demo/tsconfig.app.json"
          }
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "development": {
              "buildTarget": "dfx-bootstrap-table-demo:build:development"
            },
            "production": {
              "buildTarget": "dfx-bootstrap-table-demo:build:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular/build:unit-test"
        }
      },
      "prefix": "app",
      "projectType": "application",
      "root": "apps/dfx-bootstrap-table-demo",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "sourceRoot": "apps/dfx-bootstrap-table-demo/src"
    },
